{
  "manifest_version": 3,
  "name": "Audio Tab Navigator",
  "version": "1.1.0",
  "description": "音声を出力しているタブを素早く見つけ、切り替えやクローズを行うための拡張機能です。",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "audio-tab-navigator@example.com",
      "strict_min_version": "109.0"
    }
  }
}